{
  "manifest_version": 2,
  "name": "Tab Changer",
  "description": "This extension helps to switch tabs easily.",
  "version": "1.0",

  "browser_action": {
    "default_popup": "linkController.html",
    "default_title": "Tab Changer."
  },  
	"icons": {
		"16": "logo16.png",
		"48": "logo48.png",
		"128": "logo128.png"
	},
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ]
}